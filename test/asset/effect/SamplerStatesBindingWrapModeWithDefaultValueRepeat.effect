{
	"uniforms"   : {
        "uDiffuseMap"   : {
            "binding"       : "material[${materialUuid}].diffuseMap",
            "wrapMode"      : { 
				"binding"       : "material[${materialUuid}].diffuseMapWrapMode" ,
				"default"		: "repeat"
			}
        }
    },

    "techniques" : [{
        "passes" : [{
            "vertexShader" : "#pragma include \"dummy.glsl\"",
            "fragmentShader" : "
                uniform sampler2D uDiffuseMap;

                void main(void)
                {
                    gl_FragColor = texture2D(uDiffuseMap, vec2(0, 0));
                }
            "
        }]
    }]
}
